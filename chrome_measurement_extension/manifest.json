{
  "manifest_version": 3,
  "name": "Measurement Converter",
  "version": "1.0",
  "description": "measurement converter for web pages",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}